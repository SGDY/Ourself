<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/check_disabled" android:state_enabled="false" />
    <!-- Due to top-down rules, pressed will always win over checked -->
    <item android:drawable="@drawable/check_pressed" android:state_pressed="true" />
    <item android:drawable="@drawable/check_checked" android:state_checked="true" />
    <!-- Default State -->
    <item android:drawable="@drawable/check_default" />
</selector>
